{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Start App",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "windows": {
        "program": "${workspaceFolder}/MyApp.Windows/bin/Debug/netcoreapp3.0/MyApp.dll",
        "cwd": "${workspaceFolder}/MyApp.Windows"
      },
      "linux": {
        "program": "${workspaceFolder}/MyApp.Linux/bin/Debug/netcoreapp3.0/MyApp.dll",
        "cwd": "${workspaceFolder}/MyApp.Linux"
      },
      "osx": {
        "program": "${workspaceFolder}/MyApp.Mac/bin/Debug/netcoreapp3.0/MyApp.dll",
        "cwd": "${workspaceFolder}/MyApp.Mac"
      },
      "args": [],
      "console": "internalConsole",
      "stopAtEntry": false,
      "internalConsoleOptions": "openOnSessionStart"
    },
    {
      "name": ".NET Core Attach",
      "type": "coreclr",
      "request": "attach",
      "processId": "${command:pickProcess}"
    }
  ]
}
