{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Start SPA Example",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build-example-spa",
            "program": "${workspaceFolder}/Examples/SpiderEye.Example.Spa/bin/Debug/netcoreapp2.2/SpiderEye.Example.Spa.dll",
            "args": [],
            "cwd": "${workspaceFolder}/Examples/SpiderEye.Example.Spa",
            "console": "internalConsole",
            "stopAtEntry": false,
            "internalConsoleOptions": "openOnSessionStart"
        },
        {
            "name": "Start Simple Example",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build-example-simple",
            "program": "${workspaceFolder}/Examples/SpiderEye.Example.Simple/bin/Debug/netcoreapp2.2/SpiderEye.Example.Simple.dll",
            "args": [],
            "cwd": "${workspaceFolder}/Examples/SpiderEye.Example.Simple",
            "console": "internalConsole",
            "stopAtEntry": false,
            "internalConsoleOptions": "openOnSessionStart"
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}
